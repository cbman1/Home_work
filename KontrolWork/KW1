import java.util.Scanner;
public class KW1 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int n = scanner.nextInt();
        pow(a,n);
    }
    static void pow (int a, int n) {
        int b = a;
        for (int i = 1; i < n; i++) {
            a *= b;
        }
        System.out.println(a);
    }
}
